    "openapi-typescript-codegen": "0.24.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "10.9.1",
    "xmlbuilder2": "3.1.1"
  }
}
